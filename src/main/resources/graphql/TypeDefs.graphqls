scalar LocalDateTime
scalar Long
scalar BigDecimal
type AccountHolder{
id:Long
name:String
gender:String
email:String
phoneNumber:String
profilePicture:String
dob:LocalDateTime
password:String
role:Role
studentList:[Student]
hospitalAdminList:[HospitalAdmin]
accountHolderSignatureList:[Certificate]
}
type Certificate {
id:Long
title:String
description:String
userSignature:String
hospitalStamp:String
